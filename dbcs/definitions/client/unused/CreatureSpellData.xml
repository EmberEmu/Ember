<?xml version="1.0"?>
<dbc>
	<name>CreatureSpellData</name>
	
	<field>
		<type>uint32</type>
		<name>id</name>
		<key>
			<type>primary</type>
		</key>
	</field>

	<field>
		<type>uint32[4]</type>
		<name>spell</name>
